@model IEnumerable<Category>
<div class="list-group">

    <a href="#" class="list-group-item list-group-item-action">ANASAYFA</a>
    @foreach (var category in Model)
    {
        <a asp-action="Index" asp-controller="Home" class="list-group-item list-group-item-action disabled" tabindex="-1" aria-disabled="true">@category.Name</a>
    }

</div>
